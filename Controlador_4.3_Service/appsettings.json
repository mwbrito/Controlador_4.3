{
  "ServiceConfig": {
    "StartHour": 12,
    "StartMinute": 45,
    "LogPath": "C:\\Dev\\",
    "ServiceListPath": "c:\\dev\\service_list.txt"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "LogStreamNamePrefix": "dev",
    "Region": "ap-south-1",
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Dev\\mqCtrreg-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30
        }
      }      
    ],
    "Properties": {
      "Application": "WorkerService.LoggingDemo"
    }
  }
}
